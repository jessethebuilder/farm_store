<p id="notice"><%= notice %></p>

<h1>Listing Farm Store Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Standard tax rate</th>
      <th>Has farm store profile</th>
      <th>Has farm store profile type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @farm_store_profiles.each do |farm_store_profile| %>
      <tr>
        <td><%= farm_store_profile.standard_tax_rate %></td>
        <td><%= farm_store_profile.has_farm_store_profile_id %></td>
        <td><%= farm_store_profile.has_farm_store_profile_type %></td>
        <td><%= link_to 'Show', farm_store_profile %></td>
        <td><%= link_to 'Edit', edit_farm_store_profile_path(farm_store_profile) %></td>
        <td><%= link_to 'Destroy', farm_store_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Farm store profile', new_farm_store_profile_path %>
